<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="userLabel" layoutX="162.0" layoutY="180.0" prefHeight="18.0" prefWidth="100.0" text="Username:" />
      <Label fx:id="passLabel" layoutX="162.0" layoutY="208.0" prefHeight="18.0" prefWidth="100.0" text="Password:" />
      <TextField fx:id="usernameText" layoutX="268.0" layoutY="177.0" />
      <TextField fx:id="passwordText" layoutX="268.0" layoutY="205.0" />
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="221.0" layoutY="14.0" prefHeight="35.0" prefWidth="157.0" text="Login" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="loginBtn" layoutX="230.0" layoutY="262.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
      <Label fx:id="locationLabel" layoutX="182.0" layoutY="123.0" text="Location:" />
      <Label fx:id="userLocationLabel" layoutX="286.0" layoutY="123.0" text="Label" textAlignment="CENTER" />
      <Button fx:id="exitBtn" layoutX="324.0" layoutY="262.0" mnemonicParsing="false" onAction="#onExit" text="Exit" />
   </children>
</AnchorPane>
